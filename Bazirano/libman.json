{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootswatch@4.3.1",
      "destination": "wwwroot/lib/bootswatch/",
      "files": [
        "darkly/bootstrap.min.css",
        "flatly/bootstrap.min.css",
        "litera/bootstrap.min.css",
        "materia/bootstrap.min.css"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js" 
      ]
    },
    {
      "library": "font-awesome@5.12.1",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-brands-400.woff2"
      ]
    },
    {
      "library": "showdown@1.9.1",
      "destination": "wwwroot/lib/showdown/",
      "files": [
        "showdown.js"
      ]
    }
  ]
}